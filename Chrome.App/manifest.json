{
  "name": "EventFeedack",
  "description": "EventFeedback",
  "version": "1.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "128": "images/icon-128x128.png"
  }, 
  "content_security_policy": "default-src 'self'; script-src 'self'; object-src 'self'",
  "minimum_chrome_version": "18",
  "permissions": [
          "storage",
          "contextMenus", "cookies", "tabs", "notifications",
          "http://*/", "https://*/"
        ],
  "sandbox": { "pages": ["index.chrome.html"] }
}