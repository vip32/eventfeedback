<div><strong>{{model.title}}</strong></div>
{{#each model.tags}}<span class="badge pull-right">{{this}}</span>{{/each}}
<div class="glyphicon glyphicon-time">
  {{dateFormat model.startDate format="HH:mm"}}-{{dateFormat model.endDate format="HH:mm"}}
</div>
<div class="glyphicon glyphicon-user">
  {{#each model.speakers}}{{this}} {{/each}}
</div>
