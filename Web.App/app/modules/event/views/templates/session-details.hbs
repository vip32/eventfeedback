<div class="container">

  <div class="list-group" style="margin-top:39px;">
    <div id="session-item-view" class="list-group-item">
      <div>
        <div class="glyphicon icon-presentation"></div>
        &emsp;&emsp;<strong>{{model.title}}</strong>
      </div>
      {{#each model.tags}}<span class="badge pull-right">{{this}}</span>{{/each}}
      <div>
      <div>
        <div class="glyphicon glyphicon-user"></div>
        &emsp;&emsp;{{seperatelist model.speakerList}}
      </div>
      <div class="glyphicon glyphicon-time"></div>
        &emsp;&emsp;{{dateFormat model.startDate format="HH:mm"}}-{{dateFormat model.endDate format="HH:mm"}}
      </div>
      {{#if model.location}}
      <div>
        <div class="glyphicon glyphicon-map-marker"></div>
        &emsp;&emsp;{{model.location}}
      </div>
      {{/if}}
      <br/><br/>
      <p>{{model.description}}</p>
    </div>
  </div>

  <form>
    {{#if feedbackdefinition.active0}}
    <div class="row">
      {{#ifCond feedbackdefinition.questionType0 0}}
      <div class="col-md-3">{{feedbackdefinition.title0}} </div>
      <div class="col-md-9">
        <textarea name="answer0" data-maxlength="2048">{{feedback.answer0}}</textarea>
      </div>
      {{/ifCond}}
      {{#ifCond feedbackdefinition.questionType0 1}}
      <div class="col-md-3">{{feedbackdefinition.title0}}</div>
      <div class="col-md-9">
        <input type="text" name="answer0" maxlength="2048" value="{{feedback.answer0}}"/>
      </div>
      {{/ifCond}}
      {{#ifCond feedbackdefinition.questionType0 10}}
      <div class="col-xs-4 col-md-3">{{feedbackdefinition.title0}}</div>
      <div class="col-xs-8 col-md-9">
        <!--<input type="number" data-max="3" data-min="1"
             name="answer0" id="answer0" class="rating" value="{{#zerowhenempty feedback.answer0}}{{/zerowhenempty}}" />-->
